// Use inheritance to inherit properties from the person class to  Oldperson class
//---Requirement 13: Extend another class---
//---Requirement 19: Inheritance---
public class Oldperson extends Person {

    private int OLDmodifier = -10;

    public Oldperson(int smoking, int education, int cancer, int alcohol, int sleep) {

        //---Requirement 16: Super Keyword---
        super();

    }

    public int getOLDmodifier() {
        return OLDmodifier;
    }

    public void setOLDmodifier(int OLDmodifier) {
        this.OLDmodifier = OLDmodifier;
    }
}
